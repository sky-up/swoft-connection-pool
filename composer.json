{
  "name": "swoft-fork/connection-pool",
  "type": "library",
  "version": "v2.1.0",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "swoft connection pool component",
  "homepage": "https://github.com/sky-up/swoft-connection-pool",
  "license": "Apache-2.0",
  "require": {
    "swoft-fork/stdlib": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Connection\\Pool\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Connection\\Pool\\Unit\\": "test/unit/",
      "SwoftTest\\Connection\\Pool\\Testing\\": "test/testing/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
